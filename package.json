{
  "name": "vuejs-portfolio",
  "version": "1.0.0",
  "description": "Modern Vue 3 Portfolio for Shahul Hameed Badhusha Ansari",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:server": "node server/index.js",
    "dev:all": "concurrently \"npm run dev\" \"npm run dev:server\"",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest run --environment jsdom",
    "test:watch": "vitest --environment jsdom",
    "test:ui": "vitest --ui",
    "lint": "echo 'Linting not configured'",
    "lint:fix": "echo 'Lint fix not configured'",
    "type-check": "vue-tsc --noEmit",
    "analyze": "vite-bundle-analyzer dist",
    "docker:build": "docker build -t portfolio-frontend .",
    "docker:run": "docker run -p 2025:80 portfolio-frontend",
    "docker:compose": "docker-compose up -d",
    "docker:compose:down": "docker-compose down",
    "deploy:staging": "echo 'Staging deployment not configured'",
    "deploy:production": "echo 'Production deployment not configured'"
  },
  "dependencies": {
    "@vueuse/motion": "^3.0.3",
    "animate.css": "^4.1.1",
    "body-parser": "^2.2.0",
    "bootstrap": "^5.3.8",
    "chart.js": "^4.4.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.2",
    "express": "^5.1.0",
    "nodemailer": "^7.0.6",
    "supertest": "^7.1.4",
    "three": "^0.180.0",
    "vue": "^3.5.21",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.0.0",
    "@vitest/ui": "^3.2.4",
    "@vue/test-utils": "^2.4.6",
    "concurrently": "^9.2.1",
    "jsdom": "^27.0.0",
    "typescript": "^5.9.2",
    "vite": "^5.0.0",
    "vitest": "^3.2.4",
    "vue-tsc": "^3.0.7"
  },
  "keywords": [
    "vue",
    "portfolio",
    "vite",
    "bootstrap"
  ],
  "author": "Shahul Hameed Badhusha Ansari",
  "license": "MIT"
}
